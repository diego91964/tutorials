CREATE TABLE CREDENCIAIS_APLICACOES (
    ID_CREDENCIAL_APLICACAO    INTEGER NOT NULL AUTO_INCREMENT,
    CREDENCIAL_APLICACAO       VARCHAR(150),
    SENHA_APLICACAO            VARCHAR(100),
    EMAIL_RESPONSAVEL          VARCHAR(30),
    NOME_RESPONSAVEL           VARCHAR(30),
    IND_ATIVO                  SMALLINT NOT NULL DEFAULT 1,
    PRIMARY KEY(ID_CREDENCIAL_APLICACAO)
)

GO

CREATE TABLE PAPEIS_APLICACOES (
    ACRONIMO              VARCHAR(50),
    DESCRICAO             VARCHAR(100),
    ID_PAPEL_APLICACAO    INTEGER NOT NULL AUTO_INCREMENT,
    PRIMARY KEY(ID_PAPEL_APLICACAO)
)
GO

CREATE TABLE PAPEIS_CREDENCIAIS (
    ID_CREDENCIAL_APLICACAO    INTEGER NOT NULL,
    ID_PAPEL_APLICACAO         INTEGER NOT NULL,
    PRIMARY KEY(ID_CREDENCIAL_APLICACAO,ID_PAPEL_APLICACAO)
)
GO
ALTER TABLE PAPEIS_CREDENCIAIS
    ADD CONSTRAINT SQL150911152834770
    FOREIGN KEY(ID_PAPEL_APLICACAO)
    REFERENCES PAPEIS_APLICACOES(ID_PAPEL_APLICACAO)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION
GO
ALTER TABLE PAPEIS_CREDENCIAIS
    ADD CONSTRAINT SQL150911152834760
    FOREIGN KEY(ID_CREDENCIAL_APLICACAO)
    REFERENCES CREDENCIAIS_APLICACOES(ID_CREDENCIAL_APLICACAO)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION
GO
